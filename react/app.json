{
  "expo": {
    "name": "Ogame Mobile",
    "slug": "ogame-mobile",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "assets/**",
      "common/**"
    ],
    "packagerOpts": {
      "assetExts": ["txt"]
    },

    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.azke.ogame_mobile",
      "permissions": []
    }
  }
}